class Color{
  
  public float red, green, blue, alpha = 0;
  
  public Color(float r, float g, float b, float a){
    red = r;
    green = g;
    blue = b;
    alpha = a;
  }
  
  public float getRed(){
    return red;
  }
  
  public float getGreen(){
    return green;
  }
  
  public float getBlue(){
    return blue;
  }
  
  public float getAlpha(){
    return alpha;
  }
}
